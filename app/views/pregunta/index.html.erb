<p id="notice"><%= notice %></p>

<h1>Listing Pregunta</h1>

<table>
  <thead>
    <tr>
      <th>Pregunta</th>
      <th>Pregunta enunciado</th>
      <th>Pregunta descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pregunta.each do |preguntum| %>
      <tr>
        <td><%= preguntum.Pregunta_id %></td>
        <td><%= preguntum.Pregunta_enunciado %></td>
        <td><%= preguntum.Pregunta_Descripcion %></td>
        <td><%= link_to 'Show', preguntum %></td>
        <td><%= link_to 'Edit', edit_preguntum_path(preguntum) %></td>
        <td><%= link_to 'Destroy', preguntum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Preguntum', new_preguntum_path %>
