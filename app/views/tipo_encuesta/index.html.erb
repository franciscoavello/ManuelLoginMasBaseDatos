<p id="notice"><%= notice %></p>

<h1>Listing Tipo Encuesta</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Tipo nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_encuesta.each do |tipo_encuestum| %>
      <tr>
        <td><%= tipo_encuestum.Tipo_id %></td>
        <td><%= tipo_encuestum.Tipo_nombre %></td>
        <td><%= link_to 'Show', tipo_encuestum %></td>
        <td><%= link_to 'Edit', edit_tipo_encuestum_path(tipo_encuestum) %></td>
        <td><%= link_to 'Destroy', tipo_encuestum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo encuestum', new_tipo_encuestum_path %>
