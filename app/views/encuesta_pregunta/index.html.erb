<p id="notice"><%= notice %></p>

<h1>Listing Encuesta Pregunta</h1>

<table>
  <thead>
    <tr>
      <th>Encuesta pregunta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encuesta_pregunta.each do |encuesta_preguntum| %>
      <tr>
        <td><%= encuesta_preguntum.Encuesta_Pregunta_id %></td>
        <td><%= link_to 'Show', encuesta_preguntum %></td>
        <td><%= link_to 'Edit', edit_encuesta_preguntum_path(encuesta_preguntum) %></td>
        <td><%= link_to 'Destroy', encuesta_preguntum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Encuesta preguntum', new_encuesta_preguntum_path %>
